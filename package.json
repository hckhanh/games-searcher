{
  "name": "games-searcher",
  "version": "0.0.1",
  "engines": {
    "node": "7.6.0"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently --prefix \"[{name}]\" --names \"server,web\" \"nodemon --config nodemon.json ./bin/www\" \"webpack-dev-server --config dev.config.js\"",
    "dev-server": "cross-env DEBUG=games-searcher:* PORT=4000 node %NODE_DEBUG_OPTION% ./bin/www",
    "build": "webpack --config prod.config.js",
    "heroku-postbuild": "npm run build",
    "start": "cross-env NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "antd": "^2.7.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "body-parser": "~1.16.0",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "es6-promise": "^4.0.5",
    "express": "~4.14.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "morgan": "~1.8.1",
    "node-fetch": "^1.6.3",
    "opbeat-react": "^2.1.7",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
